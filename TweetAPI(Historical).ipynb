   "source": [
    "client_key = '**'\n",
    "client_secret = '**'\n",
    "BEARER_TOKEN = \"**"\n",
    "\n",
    "key_secret = '{}:{}'.format(client_key, client_secret).encode('ascii')\n",
    "b64_encoded_key = base64.b64encode(key_secret)\n",
