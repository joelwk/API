   "source": [
    "#Function to create dataframe of interest\n",
    "def get_data(tweet):\n",
    "    data = {\n",
    "        'id': tweet['id'],\n",
    "        'created_at': tweet['created_at'],\n",
    "        'text': tweet['text'],\n",
    "        'lang': tweet['lang']\n",
    "        \n",
    "    }\n",
    "    return data"
   ]
  },
